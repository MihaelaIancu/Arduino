#include <LiquidCrystal.h>
#define V0_PIN 9

LiquidCrystal lcd(2, 3, 4, 5, 6, 7);

void setup() {
  // put your setup code here, to run once:
lcd.begin(16, 2);
lcd.clear();
lcd.setCursor(4, 0);
lcd.print("Catel");
//lcd.begin(16, 2);
//lcd.clear();
lcd.setCursor(2, 1);
lcd.print("cuminte");
//lcd.clear();
//lcd.home();

pinMode(V0_PIN, OUTPUT);
analogWrite(V0_PIN, 90);
}

